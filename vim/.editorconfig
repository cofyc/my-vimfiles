; https://github.com/maksimr/vim-jsbeautify
;.editorconfig

root = true

[**.js]
indent_style = space
indent_size = 2

[**.json]
indent_style = space
indent_size = 4

[**.jsx]
e4x = true
indent_style = space
indent_size = 4

[**.css]
indent_style = space
indent_size = 2

[**.html]
indent_style = space
indent_size = 2
max_char = 78
brace_style = expand
